{
    "name": "symbiote/silverstripe-prose-editor",
    "description": "A wysiwyg editor based on prose mirror",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "wysiwyg",
        "prose"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\Prose\\": "src/",
            "Symbiote\\Prose\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        },
        "expose": [
          "editor/dist",
          "client/images"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
